{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
        
    },
    "rules": {
      "import/no-cycle": "off"
    },
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".cjs"]
          }
        }
      }
}
